}
.page>.moon{
  height: 120rpx;
  width: 100%;
  background-color:white;
}
.image{
  display: inline-block;
  width: 40rpx;
  height:40rpx;
}
.text{
  display: inline-block;
  color: #66ccff;
  font-size: 30rpx;
  margin-left:10rpx;
  margin-top:-15rpx;
  line-height: 20rpx;
}
.content{
  position: relative;
  margin-left: 20rpx;
