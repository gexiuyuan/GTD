page{
  width: 100%;
  height:100%;
  background-color: 	#F5F5F5;
}
.page>.title{
  height: 300rpx;
  width: 100%;
  background-color:#00BFFF; 
}
.page>.title>.image{
  position: absolute;
  height: 150rpx;
  width: 150rpx;
  background-color: white;
  border-radius: 50%;
  margin: 80rpx 0 0 280rpx;
}
.page>.title>.image>image{
  margin: 10rpx 0 0 30rpx;
}
.page>.time {
  height: 180rpx;
  width: 100%;
  background-color:white;
}
.page>.Ebinghaus{
  height: 90rpx;
  width: 100%;
  background-color:white;
}
.page>.day{
  height: 60rpx;
  width: 100%;
  background-color:white;
}
.page>.search{
  height: 120rpx;
  width: 100%;
  background-color:white;
}
.page>.classes{
  height: 120rpx;
  width: 100%;
  background-color:white;
}
.page>.success{
  height: 120rpx;
  width: 100%;
  background-color:white;
}
.page>.child_task{
  height: 120rpx;
  width: 120%;
